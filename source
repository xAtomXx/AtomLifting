AutoF.Name = "AutoF"
AutoF.Parent = Auto
AutoF.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
AutoF.BorderSizePixel = 0
AutoF.Position = UDim2.new(0.0249376558, 0, 0.0218978096, 0)
AutoF.Size = UDim2.new(0, 116, 0, 36)
AutoF.Font = Enum.Font.SourceSans
AutoF.Text = "AutoF"
AutoF.TextColor3 = Color3.fromRGB(0, 0, 0)
AutoF.TextScaled = true
AutoF.TextSize = 14.000
AutoF.TextWrapped = true
AutoF.MouseButton1Click:connect(function()
	_G.LULU = true--change to true or false
	while _G.LULU do
		wait()--change time if you want
		if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") then
			game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):Activate()
		end
	end
end

