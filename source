AutoFnoweight.TextSize = 14.000
AutoFnoweight.TextWrapped = true
AutoFnoweight.MouseButton1Click:connect(function()
	print("AutoFnoweight succes!")
	wait(1)
	_G.Lift = true

	while _G.Lift do
		wait(0.5)
		local table_1 = {
			[1] = 'GainMuscle'
		};
		local Target = game:GetService("ReplicatedStorage").RemoteEvent;
		Target:FireServer(table_1);
	end
end)

UICorner_15.Parent = AutoFnoweight
